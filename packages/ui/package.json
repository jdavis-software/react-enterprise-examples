{
  "name": "@react-enterprise-examples/ui",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tokens.scss": "./dist/tokens.scss"
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json --emitDeclarationOnly --declaration --skipLibCheck --outDir dist && cp src/tokens.scss dist/tokens.scss && cp -r src/styles dist/styles",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "test:vis": "npm run build:storybook && npx playwright install --with-deps chromium && node test-vis.mjs"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "sass": "^1.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "jsdom": "^24.0.0",
    "storybook": "^8.1.0",
    "@storybook/react-vite": "^8.1.0",
    "@storybook/addon-essentials": "^8.1.0",
    "@storybook/addon-a11y": "^8.1.0",
    "playwright": "^1.45.0"
  }
}
